public class UIEventsManager \{\line
    private Map<String, ICommand> menuCommandsMap = new HashMap<String, ICommand>();\line
    private Map<String, ICommand> toolBarCommandsMap = new HashMap<String, ICommand>();\line
    \line
    public void addMenuCommand(String key, ICommand menuCommand) \{\line
        this.menuCommandsMap.put(key, menuCommand);\line
    \}\line
    \line
    public void addToolBarCommand(String key, ICommand toolbarCommand) \{\line
        this.toolBarCommandsMap.put(key, toolbarCommand);\line
    \}\line
    \line
    public void handleMenuPressEvent(String key) \{\line
        ICommand menuCommand = this.menuCommandsMap.get(key);\line
        if (menuCommand != null) \{\line
            menuCommand.execute();\line
        \}\line
    \}\line
    \line
    public void handleUndoMenuPressEvent(String key) \{\line
        ICommand menuCommand = this.menuCommandsMap.get(key);\line
        if (menuCommand != null) \{\line
            menuCommand.undo();\line
        \}\line
    \}\line
    \line
    public void handleToolBarPressEvent(String key) \{\line
        ICommand toolbarCommand = this.toolBarCommandsMap.get(key);\line
        if (toolbarCommand != null) \{\line
            toolbarCommand.execute();\line
        \}\line
    \}\line
    \line
    public void handleUndoToolBarPressEvent(String key) \{\line
        ICommand toolbarCommand = this.toolBarCommandsMap.get(key);\line
        if (toolbarCommand != null) \{\line
            toolbarCommand.undo();\line
        \}\line
    \}\line
\}\par}

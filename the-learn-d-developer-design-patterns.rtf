// IDocumentOperations acts as a "Receiver" (Command Pattern)\line
public interface IDocumentOperations \{\line
    public void open(String fileName);\line
    public void close(String fileName);\line
    public void cut();\line
    public void undoPaste();\line
    public void paste();\line
\}\line
\line
public class DocumentOperations implements IDocumentOperations \{\line
\line
    public void open(String fileName) \{\line
        System.out.println("Opening " + fileName + "...");\line
    \}\line
\line
    public void close(String fileName) \{\line
        System.out.println("Closing " + fileName + "...");\line
    \}\line
\line
    @Override\line
    public void cut() \{\line
